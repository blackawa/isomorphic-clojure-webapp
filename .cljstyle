{;; Legacy config for cljstyle, but required by cljfmt bundled in Calva...
 :indents {bind-context [[:block 1]]
           require [[:inner 0]]}
 :rules {:namespaces {:break-libs? false}
         :whitespace {:remove-surrounding? true
                      :remove-trailing? true
                      :insert-missing? true}
         :blank-lines {:trim-consecutive? false}}}